---
import Layout from "./Layout.astro";
import Navbar from "../components/Navbar.astro";
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
    <Navbar></Navbar>

    <main class="container m-auto p-[1em] max-w-[175ch]">
        <slot></slot>
    </main>
</Layout>